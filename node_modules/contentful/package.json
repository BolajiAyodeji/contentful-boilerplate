    "build:modules": "BABEL_ENV=webpack babel lib -d dist/es6-modules && cp ./version.js ./dist/version.js",
    "build:standalone": "BABEL_ENV=webpack webpack && BABEL_ENV=webpack NODE_ENV=production webpack",
